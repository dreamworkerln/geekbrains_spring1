package jsonrpc.server.entities.base.mapper;

import jsonrpc.protocol.dto.base.jrpc.AbstractDto;
import jsonrpc.server.entities.base.AbstractEntity;
import org.mapstruct.Mapper;
import org.mapstruct.factory.Mappers;

@Mapper(componentModel = "spring",
        uses = {InstantLongMapper.class})

public interface AbstractMapper2 {
    AbstractMapper2 INSTANCE = Mappers.getMapper(AbstractMapper2.class);

    AbstractEntity toDto(AbstractDto dto);
    AbstractDto toEntity(AbstractEntity entity);
}
