package jsonrpc.server.entities.order.request;

import jsonrpc.server.entities.base.param.AbstractParam;
import jsonrpc.server.entities.order.Order;
import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Component;

@Component
@Scope("prototype")
public class PutOrder extends AbstractParam {

    private Order order;

    @Override
    public void validate() {

        if (order == null) {
            throw new IllegalArgumentException("Error parsing request: id <= 0");
        }
    }


}
